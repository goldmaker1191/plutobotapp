{
  "name": "plutobot",
  "version": "1.0.0",
  "description": "PlutoBot",
  "author": "Nisthar <nistharkalathilk@gmail.com>",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "nuxt build",
    "start": "babel-node server/server.js",
    "heroku-postbuild": "npm run build",
    "local": "nodemon --exec babel-node server.js --watch server.js --watch ./routes --watch ./controllers --watch ./api --watch ./middleware --watch ./models --watch ./config -e js ",
    "heroku": "git add . && git commit -am \"ignore node_modules\" && git push heroku master && pause",
    "dev": "backpack dev",
    "nuxt": "nuxt start",
    "sync": "browser-sync start --proxy localhost:3000 --files views/**/*.ejs",
    "prod": "export NODE_ENV='production'",
    "DO": "PORT=8080 NODE_ENV='production' pm2 start server.js --interpreter ./node_modules/.bin/babel-node",
    "dopush": "nodemon --exec npm run bitbucket --watch server.js --watch ./routes --watch ./api --watch ./middleware --watch ./models --watch ./config -e js ",
    "bitbucket": "git add . && git commit -am \"ignore node_modules\" && git push origin master"
  },
  "dependencies": {
    "actions-on-google": "^2.3.0",
    "axios": "^0.16.2",
    "babel-cli": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.0.1",
    "dialogflow": "^0.6.0",
    "dialogflow-fulfillment": "^0.5.0",
    "ejs": "^2.6.1",
    "ejs-mate": "^2.3.0",
    "express": "^4.15.3",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "mongoose": "^5.2.8",
    "morgan": "^1.9.0",
    "nuxt": "latest",
    "passport": "^0.4.0",
    "source-map-support": "^0.4.15"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1"
  }
}
